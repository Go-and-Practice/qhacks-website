{
  "name": "qhacks-website",
  "version": "3.0.0",
  "description": "Website for QHacks!",
  "author": "QHacks Engineering",
  "homepage": "https://github.com/qhacks/qhacks-website#readme",
  "bugs": {
    "url": "https://github.com/qhacks/qhacks-website/issues"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/qhacks/qhacks-website.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "style": "eslint --quiet src/**/*.js",
    "format": "prettier --write 'src/**/*.js'",
    "test": "jest",
    "test-watch": "jest --watchAll",
    "test-with-coverage": "jest --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "engines": {
    "node": "8.11.x",
    "npm": "6.x"
  },
  "dependencies": {
    "gatsby": "^1.9.277",
    "gatsby-link": "^1.6.46",
    "gatsby-plugin-react-helmet": "^2.0.11",
    "react-helmet": "^5.2.0"
  },
  "devDependencies": {
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.4.2",
    "coveralls": "^3.0.2",
    "enzyme": "^3.5.0",
    "enzyme-adapter-react-16": "^1.3.1",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.4.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^0.14.3",
    "jest": "^23.5.0",
    "prettier": "^1.13.7",
    "pretty-quick": "^1.6.0"
  }
}
